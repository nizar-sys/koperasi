select `spkopkar`.`tbl_pinjaman_h`.`id` AS `id`,`spkopkar`.`tbl_pinjaman_h`.`itPostPinjam` AS `posting`,`spkopkar`.`tbl_pinjaman_h`.`no_pinjaman` AS `nopinjam`,`spkopkar`.`tbl_pinjaman_h`.`tgl_pinjam` AS `tgl_pinjam`,`spkopkar`.`tbl_pinjaman_h`.`anggota_id` AS `anggota_id`,`spkopkar`.`tbl_pinjaman_h`.`lama_angsuran` AS `lama_angsuran`,`spkopkar`.`tbl_pinjaman_h`.`jumlah` AS `jumlah`,`spkopkar`.`tbl_pinjaman_h`.`bunga` AS `bunga`,`spkopkar`.`tbl_pinjaman_h`.`biaya_adm` AS `biaya_adm`,`spkopkar`.`tbl_pinjaman_h`.`lunas` AS `lunas`,`spkopkar`.`tbl_pinjaman_h`.`dk` AS `dk`,`spkopkar`.`tbl_pinjaman_h`.`kas_id` AS `kas_id`,`spkopkar`.`tbl_pinjaman_h`.`user_name` AS `user_name`,(`spkopkar`.`tbl_pinjaman_h`.`jumlah` / `spkopkar`.`tbl_pinjaman_h`.`lama_angsuran`) AS `pokok_angsuran`,round(ceiling(((`spkopkar`.`tbl_pinjaman_h`.`jumlah` * `spkopkar`.`tbl_pinjaman_h`.`bunga`) / 100)),-(2)) AS `bunga_pinjaman`,round((`spkopkar`.`tbl_pinjaman_h`.`jumlah` / `spkopkar`.`tbl_pinjaman_h`.`lama_angsuran`),-(2)) AS `ags_per_bulan`,(`spkopkar`.`tbl_pinjaman_h`.`tgl_pinjam` + interval `spkopkar`.`tbl_pinjaman_h`.`lama_angsuran` month) AS `tempo`,`spkopkar`.`tbl_pinjaman_h`.`jumlah` AS `tagihan`,`spkopkar`.`tbl_pinjaman_h`.`keterangan` AS `keterangan`,`spkopkar`.`tbl_pinjaman_h`.`barang_id` AS `barang_id`,ifnull(max(`spkopkar`.`tbl_pinjaman_d`.`angsuran_ke`),0) AS `bln_sudah_angsur` from (`spkopkar`.`tbl_pinjaman_h` left join `spkopkar`.`tbl_pinjaman_d` on((`spkopkar`.`tbl_pinjaman_h`.`id` = `spkopkar`.`tbl_pinjaman_d`.`pinjam_id`))) group by `spkopkar`.`tbl_pinjaman_h`.`id`